<%- model_class = Origin -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:Id) %></th>
      <th><%= model_class.human_attribute_name(:Type) %></th>
      <th><%= model_class.human_attribute_name(:Name_of_Laboratory) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @origins.each do |origin| %>
      <tr>
        <td><%= link_to origin.id, origin_path(origin) %></td>
        <td><%= origin.origin_t %></td>
        <td><%= origin.lab_name %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      origin_path(origin), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_origin_path(origin), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      origin_path(origin),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure you want to delete the Origin?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_origin_path,
            :class => 'btn btn-primary' %>
