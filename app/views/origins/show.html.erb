<%- model_class = Origin -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Type) %>:</strong></dt>
  <dd><%= @origin.origin_t %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Name_of_Laboratory) %>:</strong></dt>
  <dd><%= @origin.lab_name %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              origins_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_origin_path(@origin), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              origin_path(@origin),
              :method => 'delete',
              :data => { :confirm => 'Are you sure you want to delete the Origin?' },
              :class => 'btn btn-danger' %>
